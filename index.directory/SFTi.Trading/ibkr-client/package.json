{
  "name": "ibkr-client",
  "version": "1.0.4",
  "description": "IBKR (Interactive Brokers) Web API Client with OAuth support",
  "main": "./dist/cjs/ibkr.client.js",
  "module": "./dist/esm/ibkr.client.js",
  "types": "./dist/esm/ibkr.client.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/ibkr.client.js",
      "import": "./dist/esm/ibkr.client.js",
      "types": "./dist/esm/ibkr.client.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run format",
    "preversion": "npm run format"
  },
  "author": "Artico",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/art1c0/ibkr-client.git"
  },
  "bugs": {
    "url": "https://github.com/art1c0/ibkr-client/issues"
  },
  "homepage": "https://github.com/art1c0/ibkr-client#readme",
  "funding": "https://buymeacoffee.com/artico",
  "keywords": [
    "ibkr",
    "interactive brokers",
    "rx",
    "rxjs",
    "websocket"
  ],
  "devDependencies": {
    "@types/node-forge": "^1.3.14",
    "@types/ws": "^8.18.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "node-forge": "^1.3.1",
    "rxjs": "^7.8.2",
    "rxwebsocket": "^1.0.7",
    "undici": "^7.15.0",
    "ws": "^8.18.3"
  },
  "files": [
    "configure.js",
    "dist/**/*"
  ]
}
